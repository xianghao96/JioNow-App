<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:orientation="horizontal"    android:id="@+id/receipt_view_adapter">    <TextView        android:id="@+id/food1"        android:layout_width="170dp"        android:layout_height="wrap_content"        android:text="Chicken Rice"        android:textColor="#040404"        android:textSize="20sp"        android:typeface="sans" />    <TextView        android:id="@+id/dish_quantity"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentEnd="true"        android:layout_alignParentTop="true"        android:layout_marginRight="200dp"        android:text="2"        android:textSize="17sp"        android:textStyle="bold" />    <TextView        android:id="@+id/dish_price"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentEnd="true"        android:layout_alignParentTop="true"        android:layout_marginEnd="90dp"        android:text="$10"        android:textSize="17sp"        android:textStyle="bold" />    <TextView        android:id="@+id/total_price_for_dish"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentEnd="true"        android:layout_alignParentTop="true"        android:layout_marginEnd="16dp"        android:text="$20"        android:textSize="17sp"        android:textStyle="bold" />    <TextView        android:id="@+id/times"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentEnd="true"        android:layout_alignParentTop="true"        android:layout_marginEnd="169dp"        android:text="x"        android:textSize="17sp" /></RelativeLayout>